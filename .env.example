# Arquivo de exemplo - copie para .env

# Project
COMPOSE_PROJECT_NAME=todolist

# App Configuration
NODE_ENV=development
PORT=3000

# Database Configuration
DB_HOST=mysql
DB_PORT=3307
DB_USER=root
